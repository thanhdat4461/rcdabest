                function GetRedacted()
                    spawn(function()
                        if 5000 >  game.Players.LocalPlayer.leaderstats.Slaps.Value then
                
                            game.StarterGui:SetCore("SendNotification",{
                                Title = "Error...";
                                Text = "Slap Requirement Not Met";
                            })
                        
                        else
                        game:GetService("StarterGui"):SetCore("SendNotification",{
                            Title = "Getting REDACTED", 
                            Text = "Please wait", 
                            Icon = "rbxassetid://9379971756" 
                        })
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[10].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[9].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[8].CFrame)
                        wait(4)
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[7].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[6].CFrame)
                        wait(4)
                       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[5].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[4].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[3].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[2].CFrame)
                        wait(4)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=(game:GetService("Workspace").PocketDimension.Doors[1].CFrame)
                        game:GetService("StarterGui"):SetCore("SendNotification",{
                            Title = "Got [REDACTED]!", -- Required
                            Text = "If you have the slap requirement you should have REDACTED now", -- Required
                            Icon = "rbxassetid://9379971756" -- Optional
                        })
                    end
                    end)
                end
                 
